<script>
    function len() {
        let move = document.getElementById('anh1');
        move.style.top = parseInt(move.style.top) - 10 + 'px';
        checkVaCham()
    }

    function xuong() {
        let move = document.getElementById('anh1');
        move.style.top = parseInt(move.style.top) + 10 + 'px';
        c1Down = parseInt(move.style.top);
        checkVaCham()
    }

    function trai() {
        let move = document.getElementById('anh1');
        move.style.left = parseInt(move.style.left) - 10 + 'px';
        checkVaCham()
    }

    function phai() {
        let move = document.getElementById('anh1');
        move.style.left = parseInt(move.style.left) + 10 + 'px';
        checkVaCham()
    }

    function moveSelection(evt) {
        switch (evt.keyCode) {
            case 37:
                trai();
                checkVaCham()
                break;
            case 39:
                phai();

                checkVaCham()
                break;
            case 40:
                xuong();
                checkVaCham()
                break;
            case 38:
                len();
                checkVaCham()
                break;
        }
    }

    function docReady() {
        window.addEventListener('keydown', moveSelection);

    }

    function checkVaCham (){
        let c1 = document.getElementById('anh1')
        let cr7 = document.getElementById('anh2')

        let c1top = parseInt(c1.style.top)
        console.log('top', c1top)
        let c1down = c1top + 80;
        console.log('down', c1down)
        let c1Left = parseInt(c1.style.left);
        let c1Right = c1Left + 100;
        console.log('left', c1Left)
        console.log('right', c1Right)
        // let cr7Top = parseInt(cr7.style.top);
        // let  cr7Down = cr7Top + 80;
        // console.log('crT', cr7Top)
        // console.log('crD', cr7Down)

        if (c1top>= 380 && c1Left >= 900 && c1Down>=400 && c1Right >= 1000 ){
            alert('Ronaldo đã được tham gia C1 năm nay!! ')
        }


        }









</script>

